<Window x:Class="Ventana.VistaMedico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ventana"
        mc:Ignorable="d"
        Title="VistaMedico" Height="450" Width="403.529">
    <Grid Margin="0,0,2.667,-0.333">
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                <GradientStop Color="#FF115467" Offset="1"/>
                <GradientStop Color="#FF8BE6FF" Offset="0.392"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Button x:Name="BotonEM" Content="Agregar Examen" HorizontalAlignment="Left" Height="59" Margin="125,50,0,0" VerticalAlignment="Top" Width="166" Click="BotonEM_Click" Background="#FF74A7F8"/>
        <Button x:Name="BotonBuscar" Content="Buscar Examen" HorizontalAlignment="Left" Height="59" Margin="125,166,0,0" VerticalAlignment="Top" Width="166" Click="BotonBuscar_Click" Background="#FF74A7F8"/>
        <Button x:Name="BotonCerrar" Content="Salir" HorizontalAlignment="Left" Height="59" Margin="200,326,0,0" VerticalAlignment="Top" Width="166" Click="BotonCerrar_Click" Background="#FFFF6C4B"/>

    </Grid>
</Window>
