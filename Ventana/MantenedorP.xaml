<Window x:Class="Ventana.MantenedorP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ventana"
        mc:Ignorable="d"
        Title="MantenedorP" Height="450" Width="800" Background="#FF0F88E4">
    <Grid>
        <Label Content="Rut" HorizontalAlignment="Left" Height="25" Margin="79,30,0,0" VerticalAlignment="Top" Width="33"/>
        <Label Content="Primer Nombre" HorizontalAlignment="Left" Height="24" Margin="25,64,0,0" VerticalAlignment="Top" Width="96"/>
        <Label Content="Segundo Nombre" HorizontalAlignment="Left" Height="24" Margin="12,105,0,0" VerticalAlignment="Top" Width="109"/>
        <Label Content="Apellido Paterno" HorizontalAlignment="Left" Height="25" Margin="393,30,0,0" VerticalAlignment="Top" Width="114"/>
        <Label Content="Apellido Materno" HorizontalAlignment="Left" Height="30" Margin="393,70,0,0" VerticalAlignment="Top" Width="114"/>
        <Label Content="Correo" HorizontalAlignment="Left" Height="30" Margin="393,105,0,0" VerticalAlignment="Top" Width="114"/>
        <TextBox x:Name="TextRut" HorizontalAlignment="Left" Height="25" Margin="133,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="92" MaxLength="8" PreviewTextInput="TextRut_PreviewTextInput"/>
        <TextBox x:Name="TextPN" HorizontalAlignment="Left" Height="25" Margin="133,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" MaxLength="50" PreviewTextInput="TextPN_PreviewTextInput"/>
        <TextBox x:Name="TextSN" HorizontalAlignment="Left" Height="25" Margin="133,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" MaxLength="50" PreviewTextInput="TextSN_PreviewTextInput"/>
        <TextBox x:Name="TextAP" HorizontalAlignment="Left" Height="25" Margin="528,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="157" MaxLength="50" PreviewTextInput="TextAP_PreviewTextInput"/>
        <TextBox x:Name="TextAM" HorizontalAlignment="Left" Height="30" Margin="528,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="157" MaxLength="50" PreviewTextInput="TextAM_PreviewTextInput"/>
        <TextBox x:Name="TextCorreo" HorizontalAlignment="Left" Height="24" Margin="528,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="152" MaxLength="50"/>
        <Button x:Name="BotonAgregar" Content="Agregar" HorizontalAlignment="Left" Height="38" Margin="91,260,0,0" VerticalAlignment="Top" Width="144" Click="BotonAgregar_Click"/>
        <Button x:Name="BotonModificar" Content="Modificar" HorizontalAlignment="Left" Height="38" Margin="324,260,0,0" VerticalAlignment="Top" Width="140" Click="BotonModificar_Click"/>
        <Button x:Name="BotonAtras" Content="Cerrar" HorizontalAlignment="Left" Height="38" Margin="609,358,0,0" VerticalAlignment="Top" Width="132" Click="BotonAtras_Click"/>
        <Button x:Name="BotonBuscar" Content="Buscar" HorizontalAlignment="Left" Height="40" Margin="609,260,0,0" VerticalAlignment="Top" Width="132" Click="BotonBuscar_Click"/>
        <Label Content="-" HorizontalAlignment="Left" Height="25" Margin="230,30,0,0" VerticalAlignment="Top" Width="16"/>
        <TextBox x:Name="TextDV" HorizontalAlignment="Left" Height="25" Margin="251,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="29" MaxLength="1" PreviewTextInput="TextDV_PreviewTextInput"/>

    </Grid>
</Window>
